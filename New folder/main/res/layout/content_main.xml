<?xml version="1.0" encoding="utf-8"?>
<!--RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:background="#00032B"
    android:id="@+id/content_main"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.l.version.MainActivity"
    tools:showIn="@layout/app_bar_main"-->

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#00032B"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentTop="true"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:layout_marginRight="10dp"
    android:layout_marginEnd="10dp">
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#00032B"
            android:weightSum="1">

<RelativeLayout
    android:layout_width="70dp"
    android:layout_marginTop="40dp"
android:layout_centerInParent="true"

    android:layout_height="70dp"
    android:id="@+id/live_logo"
        android:layout_centerVertical="true"
    android:layout_centerHorizontal="true"
    android:layout_below="@+id/visnoetic_logo"
    android:layout_alignRight="@+id/date_version"
    android:layout_alignEnd="@+id/date_version">
    <com.github.clans.fab.FloatingActionButton
        android:layout_width="400dp"
        android:layout_height="400dp"
            android:layout_centerHorizontal="true"
        app:fabSize="auto"
        android:id="@+id/live_events"
        android:src="@drawable/gola_1"
        android:layout_alignParentTop="true" />
</RelativeLayout>

    <com.github.clans.fab.FloatingActionMenu
        android:id="@+id/social_floating_menu"
        android:layout_width="20dp"
        android:layout_height="50dp"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="130dp"
        android:layout_marginRight="20dp"
        android:layout_alignParentRight="true"
        fab:menu_animationDelayPerItem="55"
        fab:menu_backgroundColor="@color/transparent"
        fab:menu_buttonSpacing="5dp"
        fab:menu_colorNormal="#ffffff"
        fab:menu_colorPressed="#dfacb1"
        fab:menu_colorRipple="#99d4d4d4"
        fab:menu_fab_label="Events"
        app:menu_fab_size="normal"
        fab:menu_icon="@drawable/event"
        fab:menu_labels_colorNormal="#ffffff"
        fab:menu_labels_colorPressed="#ffffff"
        fab:menu_labels_colorRipple="#efecec"
        fab:menu_labels_cornerRadius="3dp"
        fab:menu_labels_ellipsize="none"
        fab:menu_labels_hideAnimation="@anim/fab_slide_out_to_right"
        fab:menu_labels_margin="0dp"
        fab:menu_labels_maxLines="-1"
        fab:menu_labels_padding="8dp"
        fab:menu_labels_position="left"
        fab:menu_labels_showAnimation="@anim/fab_slide_in_from_right"
        fab:menu_labels_showShadow="true"
        fab:menu_labels_singleLine="false"
        fab:menu_labels_textColor="#ffffff"
            fab:menu_labels_textSize="15sp"
            fab:menu_openDirection="up"
        fab:menu_shadowColor="#66aff1"
        fab:menu_shadowRadius="4dp"
        fab:menu_shadowXOffset="1dp"
        fab:menu_shadowYOffset="4dp"
        fab:menu_showShadow="true">

        <com.github.clans.fab.FloatingActionButton
            android:src="@drawable/online"
            app:fab_label="Online"
            android:id="@+id/online"
            android:background="@color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_size = "normal"
            />
        <com.github.clans.fab.FloatingActionButton
            android:src="@drawable/submission"
            app:fab_label="Submission"
            android:id="@+id/submission"
            android:background="@color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_size = "normal"
            />
        <com.github.clans.fab.FloatingActionButton
            android:src="@drawable/onstage"
            android:id="@+id/onstage"
            fab:fab_label="Onstage Events"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_size = "normal"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
        <com.github.clans.fab.FloatingActionButton
            android:src="@drawable/offstage"
            android:id="@+id/offStage"
            app:fab_label="Offstage Events"
            android:background="@color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_size = "normal"
            />
    </com.github.clans.fab.FloatingActionMenu>
         <RelativeLayout
             android:layout_centerHorizontal="true"
             android:layout_width="match_parent"
             android:layout_height="50dp"
             android:id="@+id/relativeLayout"
             android:layout_marginLeft="17dp"
             android:layout_marginStart="17dp"
             android:layout_below="@+id/social_floating_menu"
             android:layout_alignLeft="@+id/version_logo"
             android:layout_alignStart="@+id/version_logo">
    <TextView
    android:layout_width="match_parent"
    android:layout_height="100dp"
        android:text="NATIONAL INSTITUTE OF TECHNOLOGY
                         TIRUCHIRAPALLI"
        android:textSize="15dp"
        android:textColor="#Ffffff"
        android:id="@+id/textView5"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true" />
         </RelativeLayout>
            <RelativeLayout
    android:id="@+id/visnoetic_logo"
                android:layout_width="180dp"
    android:layout_height="80dp"
                android:layout_centerHorizontal="true"
                android:foregroundGravity="center"
                android:gravity="center"
                android:layout_marginLeft="34dp"
                android:layout_marginStart="34dp"
                android:layout_below="@+id/date_version"
                android:layout_alignLeft="@+id/relativeLayout"
                android:layout_alignStart="@+id/relativeLayout"
                android:layout_marginTop="98dp"
                android:layout_toStartOf="@+id/social_floating_menu"
                android:layout_toLeftOf="@+id/social_floating_menu">
    <ImageView
        android:id="@+id/visnoetic"
        android:layout_width="match_parent"
        android:foregroundGravity="center"
        android:paddingBottom="-40dp"
        android:paddingRight="-20dp"
        android:paddingLeft="-20dp"
        android:paddingStart="-20dp"
        android:paddingEnd="-20dp"
        android:paddingTop="-20dp"
        android:padding="-20dp"

        android:layout_height="100dp"
        fab:srcCompat="@drawable/new_visnoetic_2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
<RelativeLayout
    android:layout_width="80dp"
    android:id="@+id/edition"
    android:gravity="center"
    android:paddingLeft="-100dp"
    android:paddingRight="-100dp"
    android:foregroundGravity="center"
    android:layout_height="100dp"
    android:layout_below="@+id/date_version"
    android:layout_centerHorizontal="true">
    <ImageView
        android:id="@+id/v_logo"
        android:layout_width="80dp"
        android:paddingRight="-5000dp"
        android:paddingLeft="-5000dp"
        android:layout_height="150dp"
        fab:srcCompat="@drawable/vlogo"
        android:layout_alignParentBottom="true" />
    </RelativeLayout>
<RelativeLayout
    android:gravity="center"
    android:layout_width="match_parent"
    android:layout_height="80dp">
    <ImageView
        android:id="@+id/nitt"
        android:layout_width="150dp"
        android:layout_height="80dp"
        fab:srcCompat="@drawable/nit_logo"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    </RelativeLayout>

            <RelativeLayout
                android:gravity="center"
                android:id="@+id/date_version"
                android:layout_width="100dp"
                android:foregroundGravity="center"
                android:layout_height="50dp"
                android:layout_below="@+id/version_logo"
                android:layout_centerHorizontal="true">
    <ImageView
        android:foregroundGravity="center"
        android:id="@+id/date"
        android:layout_width="100dp"
        android:layout_height="90dp"
        fab:srcCompat="@drawable/date"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_height="100dp"
                android:layout_width="300dp"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_centerInParent="true"
                android:id="@+id/version_logo"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="34dp">
    <ImageView
        android:id="@+id/version"
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:maxHeight="42dp"
        android:layout_centerHorizontal="true"
        android:maxWidth="42dp"
        android:paddingEnd="-30dp"
        android:paddingLeft="-30dp"
        android:paddingRight="0dp"
        fab:srcCompat="@drawable/new_version_1"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="21dp" />
            </RelativeLayout>
</RelativeLayout>
        </ScrollView>
    </RelativeLayout>
<!--/RelativeLayout-->
